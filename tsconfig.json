{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es2016",
      "dom"
    ],
    "types": [
      "react",
      "jest"
    ],
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "strict": true,
    "target": "es2015",
    "baseUrl": "./src",
    "paths": {
      "interfaces": [
        "./interfaces"
      ],
      "components": [
        "./components"
      ],
      "config": [
        "./config"
      ],
      "ums": [
        "./ums"
      ],
      "utils": [
        "./utils"
      ],
      "containers": [
        "./containers"
      ],
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}